{
  "name": "mac-calculator-clone",
  "version": "1.0.0",
  "description": "clone mac calculator using react typescript, redux, and unit test using jest - react testing library",
  "main": "index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letenote/mac-calculator-clone.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/letenote/mac-calculator-clone/issues"
  },
  "homepage": "https://github.com/letenote/mac-calculator-clone#readme",
  "scripts": {
    "dev": "rm -rf dist && cross-env NODE_ENV=development webpack-dev-server --open",
    "staging": "rm -rf build && npm run prod && serve -s -p 8989 build",
    "prod": "rm -rf build && cross-env NODE_ENV=production webpack",
    "build-serve": "rm -rf build && npm run prod && serve -s -p 8989",
    "test": "jest --env=jsdom --updateSnapshot --watchAll",
    "test:coverage": "jest --env=jsdom --updateSnapshot --collectCoverage",
    "test:coverage-open": "open src/__coverage__/lcov-report/index.html",
    "test:clean": "rm -r src/__snapshots__ && rm -r src/__coverage__"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "globals": {
      "window": {}
    },
    "snapshotFormat": {
      "printBasicPrototype": false
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "testRegex": "(/watch/web/__tests__/.*|(\\.|/)(test|spec))\\.(js?|jsxxxx?|ts?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/FileMock.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/bower_components/",
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.tsx"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      "interface",
      "interfaces",
      "action-types",
      "jestGlobalMocks.ts",
      ".module.ts",
      "<rootDir>/src/test-utils.tsx",
      "<rootDir>/src/redux/store.tsx",
      "<rootDir>/src/redux/reducers/index.tsx",
      "<rootDir>/src/redux/middleware/logger.tsx",
      "<rootDir>/src/config/disableReactDevTools.tsx",
      ".mock.ts"
    ],
    "coverageDirectory": "<rootDir>/src/__coverage__/",
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "dependencies": {
    "bundle-stats-webpack-plugin": "^3.2.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/react-test-renderer": "^17.0.1",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.0",
    "jest-environment-jsdom": "^27.5.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "path": "^0.12.7",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^12.4.0",
    "serve": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-manifest-plugin": "^4.1.1",
    "webpack-merge": "^5.8.0",
    "webpack-nano": "^1.1.1"
  }
}
